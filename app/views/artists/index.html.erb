<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
    <li><%= link_to artist.name, artist_path(artist) %></li>
    <li><%= link_to artist.name, artist_songs_path(artist) %></li>
    <% artist.songs.each do |song| %>
      <li><%= link_to song.title, artist_song_path(song.artist_id, song.id) %></li>
      <li><%= link_to song.title, song_path(song.id) %></li>
    <% end %>
  <% end %>
  </ul>
  
